<ul class="allDatabasesLinks  list-unstyled">
    <% if (subjects.length > 0 ) { %>
    <% subjects.forEach(item => { %>
    <% if (item !== null) { %>
    <li class="my-3">
        <a href="https://libguides.lib.miamioh.edu/az.php?s=<%= item.id %>">
            All <%= item.name %> Databases
        </a>
    </li>
    <% } else { %>
    No relevant databases identified
    <% } %>
    <% }) %>
    <% } else { %>

    <% } %>
</ul>