<%- include ('header') %>
<link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
<script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>

<script>
    $(document).ready(function() {
        $('.select2').select2();
    });
</script>
<style>

</style>

<form action="./preview">
    <select class="select2" name="subject">
        <% subjects
            .forEach((subj) => { %>
            <% let code = "" %>
                <% 
                if (subj.majorCode) { 
                    code = 'majorCode-' + subj.majorCode 
                } else if (subj.regCode) { 
                    code = 'regCode-' + subj.regCode
                } else if (subj.deptCode) { 
                    code = 'deptCode-' + subj.deptCode 
                }
                %>
            <option value="<%= code %>"><%= subj.name %> <%= subj.majorCode %></option>
            <% }) %>
      </select>
      <input type="submit" />
</form>



<%- /* include('partials/subject-info', {subj: subj}) */ %>
<%- include ('footer') %>